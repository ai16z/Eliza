{
    "name": "@jawk/utils",
    "version": "0.0.1",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist",
        "data"
    ],
    "include": [
        "src/**/*"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "cheerio": "^1.0.0",
        "ts-node": "10.9.2",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "8.3.5"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
        "dev": "tsup src/index.ts --format esm --dts --watch",
        "lint": "eslint --fix  --cache ."
    }
}
